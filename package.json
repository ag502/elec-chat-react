  "scripts": {
    "start:dev": "concurrently -p \"[{name}]\" -n \"main,render,electron\" -c \"#106309,#2593C1,#9AE6F7\" \"npm run build-main:dev\" \"npm run start:render\" \"npm run start:electron\"",
